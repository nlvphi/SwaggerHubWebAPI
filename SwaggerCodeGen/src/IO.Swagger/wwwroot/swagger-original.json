              "schema" : {
                "$ref" : "#/components/schemas/AppraisalRequestViewModel"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Success. Return the new Vehicle Appraisal object"
          },
          "400" : {
            "description" : "There is some error while processing Vehicle Appraisal. Follow message instruction for more information"
          },
          "404" : {
            "description" : "Could not find the appraisal/vehicle/contact to update. Follow message instruction for more information"
          }
        }
